[global_config]
  enabled_plugins = LaunchpadCodeURLHandler, APTURLHandler, LaunchpadBugURLHandler
  title_transmit_bg_color = "#<%= scope.lookupvar('dotfiles::params::theme_color') %>"
[keybindings]
  reset = None
  next_tab = <Ctrl>Page_Down
  resize_up = None
  resize_left = None
  move_tab_right = None
  reset_clear = None
  move_tab_left = None
  resize_right = None
  resize_down = None
[profiles]
  [[default]]
    background_darkness = 0.76
    scrollback_lines = 1000
    scroll_on_output = False
    background_type = transparent
    scrollback_infinite = True
[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = default
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
